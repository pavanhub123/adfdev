{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-pavan-sqltrip"
		},
		"AzureBlobStoragels_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStoragels'"
		},
		"AzureDataLakeStorageDatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageDatalake'"
		},
		"AzureSqlDatabasels_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabasels'"
		},
		"AzureDataLakeStorageDatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pavangen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-pavan-sqltrip_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStoragels_connectionString": {
						"value": "[parameters('AzureBlobStoragels_connectionString')]"
					},
					"AzureDataLakeStorageDatalake_accountKey": {
						"value": "[parameters('AzureDataLakeStorageDatalake_accountKey')]"
					},
					"AzureSqlDatabasels_connectionString": {
						"value": "[parameters('AzureSqlDatabasels_connectionString')]"
					},
					"AzureDataLakeStorageDatalake_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageDatalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}