{
	"name": "SmartFoodsDelimitedTextBlob",
	"properties": {
		"linkedServiceName": {
			"referenceName": "wwilabblob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"folder": {
				"type": "string"
			},
			"file": {
				"type": "string"
			},
			"filetype": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@{dataset().file}.@{dataset().filetype}",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"container": "smartfoodstaging"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}