{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "bigdatalabdatafactory3025"
		},
		"BlobStorageOutput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageOutput'"
		},
		"Blobstroragefor2mintriggerLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blobstroragefor2mintriggerLS'"
		},
		"OnPremServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremServer'"
		},
		"blobstrDestifor2mintrgLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobstrDestifor2mintrgLS'"
		},
		"OnPremServer_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\PavanVm1\\Data\\FlightsAndWeather"
		},
		"OnPremServer_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "pavanRG1"
		},
		"Trigger_4m0_properties_CopyOnPrem2AzurePipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().scheduledTime"
		},
		"Trigger_d5m_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_d5m_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "bigdatalabdatafactory3025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Blobstroragefor2mintriggerLS_connectionString": {
						"value": "[parameters('Blobstroragefor2mintriggerLS_connectionString')]"
					},
					"blobstrDestifor2mintrgLS_connectionString": {
						"value": "[parameters('blobstrDestifor2mintrgLS_connectionString')]"
					},
					"BlobStorageOutput_connectionString": {
						"value": "[parameters('BlobStorageOutput_connectionString')]"
					},
					"OnPremServer_password": {
						"value": "[parameters('OnPremServer_password')]"
					},
					"OnPremServer_properties_typeProperties_host": {
						"value": "[parameters('OnPremServer_properties_typeProperties_host')]"
					},
					"OnPremServer_properties_typeProperties_userId": {
						"value": "[parameters('OnPremServer_properties_typeProperties_userId')]"
					},
					"Trigger_d5m_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_d5m_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_d5m_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_d5m_properties_pipeline_parameters_windowEnd')]"
					},
					"Trigger_4m0_properties_CopyOnPrem2AzurePipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_4m0_properties_CopyOnPrem2AzurePipeline_parameters_windowStart')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}